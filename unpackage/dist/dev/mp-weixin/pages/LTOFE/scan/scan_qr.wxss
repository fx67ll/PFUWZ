.flex-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.text-ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.text-ellipsis-two {
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.status_bar {
  height: 25px;
  width: 100%;
  z-index: 10;
}
.top-nav {
  width: 100%;
  height: 88rpx;
  position: fixed;
  top: 0;
  background-color: #ffffff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: 10;
}
.top-nav .top-nav-back {
  width: 25%;
}
.top-nav .top-nav-back .top-nav-back-icon {
  padding: 15rpx 15rpx 15rpx 30rpx;
}
.top-nav .top-nav-title {
  width: 45%;
  text-align: center;
  font-size: 34rpx;
  color: #000000;
}
.top-nav .top-nav-btn {
  width: calc(25% - 30rpx);
  text-align: right;
  margin-right: 30rpx;
  font-size: 28rpx;
  color: #838383;
  position: relative;
  top: 3rpx;
}
.top-nav-fake {
  width: 100%;
  height: 88rpx;
}
.top-nav-search {
  width: 100%;
  height: 80rpx;
  background-color: #ffffff;
  position: fixed;
  top: 88rpx;
  z-index: 10;
}
.top-nav-search .nav-search-box {
  width: calc(100% - 30rpx * 2);
  height: 72rpx;
  background-color: #F8F8F8;
  margin: calc(80rpx - 72rpx) auto 0 auto;
  border-radius: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.top-nav-search .nav-search-box .nav-search-icon {
  position: relative;
  left: 26rpx;
  top: -2rpx;
}
.top-nav-search .nav-search-box .nav-search-input {
  width: calc(100% - 80rpx);
  height: 100%;
  font-size: 24rpx;
}
.top-nav-search .nav-search-box .nav-input-placeholder {
  font-size: 28rpx;
  color: #c8c8c8;
}
.nav-search-box-invoice {
  width: calc(100% - 30rpx * 2);
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.nav-search-box-invoice .nav-search-box {
  width: calc(100% - 140rpx);
}
.nav-search-box-invoice .nav-search-filter {
  width: 140rpx;
  font-size: 28rpx;
  color: #313131;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
  top: 10rpx;
}
.nav-search-box-invoice .nav-search-filter .nav-search-filter-text {
  margin-right: 8rpx;
}
.nav-search-box-invoice .nav-search-filter .nav-search-filter-icon {
  margin-right: 20rpx;
  position: relative;
  top: 1rpx;
}
.top-nav-tab {
  width: 100%;
  height: 100rpx;
  background-color: #ffffff;
  position: fixed;
  top: calc(88rpx + 80rpx);
  z-index: 10;
}
.top-nav-tab .nav-tab-box {
  margin: 0 auto;
}
.top-nav-tab .nav-tab-box-two {
  width: 75%;
}
.top-nav-tab .nav-tab-box-four {
  width: 100%;
}
.top-nav-tab .nav-tab-box-three {
  width: 100%;
}
.top-nav-fake-high {
  width: 100%;
  height: calc(88rpx + 80rpx + 100rpx);
}
.top-nav-fake-middle {
  width: 100%;
  height: calc(88rpx + 80rpx);
}
.scan-box {
  width: 100%;
  height: auto;
  background-color: #F8F8F8;
  padding-bottom: 68rpx;
}
.scan-box .common-box .status-bar-wx {
  height: var(--statusbarheight);
  width: 100%;
  background-color: #ffffff;
  position: fixed;
  top: 0;
  z-index: 8;
}
.scan-box .common-box .status-bar-wx-fake {
  height: var(--statusbarheight);
  width: 100%;
}
.scan-box .common-box .top-nav {
  top: var(--statusbarheight);
}
.scan-box .common-box .page-bg {
  width: 100%;
  height: var(--clientheight);
  position: fixed;
  top: 0;
  background-color: #F8F8F8;
  z-index: -1;
}
.scan-box .html5-qr-code {
  width: 100%;
  height: var(--clientheight);
  position: fixed;
  top: 0;
}

